# Environment variables for GoogleSync.js

# Google API OAuth2 credentials
GOOGLE_CLIENT_ID=YOUR_CLIENT_ID
GOOGLE_CLIENT_SECRET=YOUR_CLIENT_SECRET
GOOGLE_REDIRECT_URI=YOUR_REDIRECT_URI

# Optional: Specify default paths or identifiers for Google services
DEFAULT_DRIVE_PATH=remote-data.json
DEFAULT_SHEETS_ID=your_default_sheet_id
DEFAULT_CALENDAR_ID=your_default_calendar_id

# Environment setting (development, production)
NODE_ENV=development
